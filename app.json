{
  "expo": {
    "name": "Estes Park Guide",
    "slug": "estes-park-guide",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.estespark",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs your location to show nearby trails.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs your location to show nearby trails.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.yourname.estespark",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "@RNMAPBOX_DOWNLOAD_TOKEN",
          "RNMapboxMapsVersion": "10.13.2"
        }
      ],
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "bf7e7caf-5b5a-48b9-978e-33654726f480"
      }
    }
  }
}
