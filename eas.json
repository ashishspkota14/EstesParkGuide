{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Debug",
          "podInstallArgs": ["--repo-update"],
        "env": {
          "RNMAPBOX_DOWNLOAD_TOKEN": "@RNMAPBOX_DOWNLOAD_TOKEN"
        }
      },
      "env": {
        "npm_config_legacy_peer_deps": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "env": {
          "RNMAPBOX_DOWNLOAD_TOKEN": "@RNMAPBOX_DOWNLOAD_TOKEN"
        }
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "env": {
          "RNMAPBOX_DOWNLOAD_TOKEN": "@RNMAPBOX_DOWNLOAD_TOKEN"
        }
      }
    }
  }
}
