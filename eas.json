{
  "cli": {
    "version": ">= 3.36.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "RNMAPBOX_DOWNLOAD_TOKEN": "RNMAPBOX_DOWNLOAD_TOKEN"
      },
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "RNMAPBOX_DOWNLOAD_TOKEN": "RNMAPBOX_DOWNLOAD_TOKEN"
      }
    },
    "production": {
      "distribution": "store",
      "env": {
        "RNMAPBOX_DOWNLOAD_TOKEN": "RNMAPBOX_DOWNLOAD_TOKEN"
      }
    }
  }
}